#!/bin/bash
if (($# < 3))
then
  echo "usage:"
  echo "   run_all [repetitions] [planning_iteration] [k]"
else
  start=`date +%s`
  #hi spread
  ./run_world 3 4 $1 $2 $3 HIGH_ "(0;0)" "(12;7)" "(14;10)" "(1;1)" "(13;9)"
  ./run_world 3 5 $1 $2 $3 HIGH_ "(0;0)" "(12;7)" "(14;10)" "(1;1)" "(13;9)"
  ./run_world 5 5 $1 $2 $3 HIGH_ "(0;0)" "(10;8)" "(7;11)" "(12;9)" "(11;0)"
  ./run_world 9 4 $1 $2 $3 HIGH_ "(0;0)" "(11;4)" "(10;14)" "(4;11)" "(0;8)"
  ./run_world 9 5 $1 $2 $3 HIGH_ "(0;0)" "(11;4)" "(10;14)" "(4;11)" "(0;8)"
  ./run_world 10 4 $1 $2 $3 HIGH_ "(0;0)" "(3;8)" "(1;12)" "(10;14)" "(9;14)"
  ./run_world 10 6 $1 $2 $3 HIGH_ "(0;0)" "(3;8)" "(1;12)" "(10;14)" "(9;14)"
  ./run_world 12 4 $1 $2 $3 HIGH_ "(0;0)" "(9;13)" "(12;3)" "(8;4)" "(0;13)"
  ./run_world 14 6 $1 $2 $3 HIGH_ "(0;0)" "(13;0)" "(6;14)" "(1;8)" "(14;1)"
  ./run_world 15 4 $1 $2 $3 HIGH_ "(0;0)" "(8;7)" "(14;1)" "(0;1)" "(3;6)"
  ./run_world 17 4 $1 $2 $3 HIGH_ "(0;0)" "(8;10)" "(2;1)" "(0;13)" "(12;1)"
  ./run_world 17 6 $1 $2 $3 HIGH_ "(0;0)" "(8;10)" "(2;1)" "(0;13)" "(12;1)"
  ./run_world 18 4 $1 $2 $3 HIGH_ "(0;0)" "(0;7)" "(6;7)" "(1;8)" "(1;1)"
  #lo spread
  ./run_world 0 5 $1 $2 $3 LOW_ "(0;0)" "(14;1)" "(7;9)" "(14;9)" "(6;4)"
  ./run_world 1 5 $1 $2 $3 LOW_ "(0;0)" "(2;13)" "(9;7)" "(13;12)" "(1;12)"
  ./run_world 4 6 $1 $2 $3 LOW_ "(0;0)" "(8;6)" "(4;13)" "(12;14)" "(12;1)"
  ./run_world 6 6 $1 $2 $3 LOW_ "(0;0)" "(7;11)" "(11;1)" "(10;4)" "(13;1)"
  ./run_world 7 5 $1 $2 $3 LOW_ "(0;0)" "(8;14)" "(11;14)" "(6;10)" "(2;7)"
  ./run_world 8 6 $1 $2 $3 LOW_ "(0;0)" "(10;9)" "(12;5)" "(13;7)" "(1;3)"
  ./run_world 9 6 $1 $2 $3 LOW_ "(0;0)" "(11;4)" "(10;14)" "(4;11)" "(0;8)"
  ./run_world 10 5 $1 $2 $3 LOW_ "(0;0)" "(3;8)" "(1;12)" "(10;14)" "(9;14)"
  ./run_world 16 4 $1 $2 $3 LOW_ "(0;0)" "(1;5)" "(3;9)" "(4;10)" "(9;14)"
  ./run_world 18 6 $1 $2 $3 LOW_ "(0;0)" "(0;7)" "(6;7)" "(1;8)" "(1;1)"

  end=`date +%s`
  runtime=$((end-start))
  echo "Done. Exec time: $runtime"
fi